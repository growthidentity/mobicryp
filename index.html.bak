<!-- Add a script section at the end of the slide for the calculator functionality -->
<script>
    // Initialize the calculator when this slide becomes active
    document.addEventListener('DOMContentLoaded', function() {
        // Hub data
        const hubTypes = {
            hub1: { 
                price: 55,
                maxCapacity: 300, 
                roi: 0.33
            },
            hub2: { 
                price: 110,
                maxCapacity: 600, 
                roi: 0.33
            },
            hub3: { 
                price: 275,
                maxCapacity: 1500, 
                roi: 0.33
            },
            hub4: { 
                price: 550,
                maxCapacity: 3000, 
                roi: 0.33
            },
            hub5: { 
                price: 1100,
                maxCapacity: 6000, 
                roi: 0.33
            },
            hub6: { 
                price: 2750,
                maxCapacity: 15000, 
                roi: 0.33
            },
            hub7: { 
                price: 5500,
                maxCapacity: 30000, 
                roi: 0.33
            },
            hub8: { 
                price: 11000,
                maxCapacity: 60000, 
                roi: 0.33
            },
            hub9: { 
                price: 27500,
                maxCapacity: 150000, 
                roi: 0.33
            },
            hub10: { 
                price: 55000,
                maxCapacity: 300000, 
                roi: 0.33
            },
            hub11: { 
                price: 110000,
                maxCapacity: 600000, 
                roi: 0.33
            },
            hub12: { 
                price: 550000,
                maxCapacity: 5000000, 
                roi: 0.33
            },
            hub13: { 
                price: 1100000,
                maxCapacity: 10000000, 
                roi: 0.33
            }
        };
        
        // Initialize calculator
        initHubCalculator();
        
        function initHubCalculator() {
            // DOM elements
            const mintingTypeToggle = document.getElementById('minting-type-toggle');
            const selfMintingInfo = document.getElementById('selfMintingInfo');
            const autoMintingInfo = document.getElementById('autoMintingInfo');
            const selfMintingResults = document.getElementById('selfMintingResults');
            const autoMintingResults = document.getElementById('autoMintingResults');
            const hubSelect = document.getElementById('hub-select');
            const amountSlider = document.getElementById('amount-slider');
            const amountDisplay = document.getElementById('amount-display');
            const sliderMaxValue = document.getElementById('slider-max-value');
            const hubPrice = document.getElementById('hub-price');
            const hubPriceAuto = document.getElementById('hub-price-auto');
            const maxCapacity = document.getElementById('max-capacity');
            const maxCapacityAuto = document.getElementById('max-capacity-auto');
            const dailyUsdt = document.getElementById('daily-usdt');
            const monthlyUsdt = document.getElementById('monthly-usdt');
            const annualUsdt = document.getElementById('annual-usdt');
            const autoMonthlyUsdt = document.getElementById('auto-monthly-usdt');
            const autoAnnualUsdt = document.getElementById('auto-annual-usdt');
            const totalUsdt = document.getElementById('total-usdt');
            
            // Tab navigation
            const tabs = document.querySelectorAll('.tab');
            const tabContents = document.querySelectorAll('.tab-content');
            
            if (!tabs.length || !tabContents.length) return;
            
            tabs.forEach(tab => {
                tab.addEventListener('click', () => {
                    const tabId = tab.getAttribute('data-tab');
                    
                    // Update active tab
                    tabs.forEach(t => t.classList.remove('active'));
                    tab.classList.add('active');
                    
                    // Show active content
                    tabContents.forEach(content => {
                        content.classList.remove('active');
                        if (content.id === `${tabId}-content`) {
                            content.classList.add('active');
                        }
                    });
                });
            });
            
            // Initialize with default values
            updateHubInfo('hub1');
            
            // Event listeners
            if (mintingTypeToggle) {
                mintingTypeToggle.addEventListener('change', function() {
                    if (this.checked) {
                        // Auto minting
                        selfMintingInfo.style.display = 'none';
                        autoMintingInfo.style.display = 'flex';
                        selfMintingResults.style.display = 'none';
                        autoMintingResults.style.display = 'block';
                    } else {
                        // Self minting
                        selfMintingInfo.style.display = 'flex';
                        autoMintingInfo.style.display = 'none';
                        selfMintingResults.style.display = 'block';
                        autoMintingResults.style.display = 'none';
                    }
                    calculateReturns();
                });
            }
            
            if (hubSelect) {
                hubSelect.addEventListener('change', function() {
                    updateHubInfo(this.value);
                    calculateReturns();
                });
            }
            
            if (amountSlider) {
                amountSlider.addEventListener('input', function() {
                    amountDisplay.textContent = this.value.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    calculateReturns();
                });
            }
            
            // Functions
            function updateHubInfo(hubType) {
                if (!hubTypes[hubType]) return;
                
                const hub = hubTypes[hubType];
                if (hubPrice) hubPrice.textContent = hub.price.toLocaleString();
                if (hubPriceAuto) hubPriceAuto.textContent = hub.price.toLocaleString();
                if (maxCapacity) maxCapacity.textContent = hub.maxCapacity.toLocaleString();
                if (maxCapacityAuto) maxCapacityAuto.textContent = hub.maxCapacity.toLocaleString();
                
                // Update slider max and display
                if (amountSlider) amountSlider.max = hub.maxCapacity;
                if (sliderMaxValue) sliderMaxValue.textContent = hub.maxCapacity.toLocaleString();
                
                // Set slider to max by default
                if (amountSlider) amountSlider.value = hub.maxCapacity;
                if (amountDisplay) amountDisplay.textContent = hub.maxCapacity.toLocaleString();
                
                calculateReturns();
            }
            
            function calculateReturns() {
                if (!hubSelect) return;
                
                const hubType = hubSelect.value;
                if (!hubTypes[hubType]) return;
                
                const hub = hubTypes[hubType];
                let amount = amountSlider ? parseFloat(amountSlider.value) || 0 : 0;
                
                const totalReturn = amount * 2.5; // 250% total ROI
                
                if (mintingTypeToggle && mintingTypeToggle.checked) {
                    // Auto minting calculations
                    const monthlyReturn = amount * 0.08; // 8% monthly
                    const annualReturn = monthlyReturn * 12;
                    
                    // Update display
                    if (autoMonthlyUsdt) autoMonthlyUsdt.textContent = monthlyReturn.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    if (autoAnnualUsdt) autoAnnualUsdt.textContent = annualReturn.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                } else {
                    // Self minting calculations
                    const dailyReturn = (amount * hub.roi / 100);
                    const monthlyReturn = dailyReturn * 30;
                    const annualReturn = dailyReturn * 365;
                    
                    // Update display
                    if (dailyUsdt) dailyUsdt.textContent = dailyReturn.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    if (monthlyUsdt) monthlyUsdt.textContent = monthlyReturn.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    if (annualUsdt) annualUsdt.textContent = annualReturn.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                }
                
                // Update total ROI display (same for both types)
                if (totalUsdt) totalUsdt.textContent = totalReturn.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
            }
        }
    });
</script>
